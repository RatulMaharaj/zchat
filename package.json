{
	"name": "zchat",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": "22"
	},
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"deploy:permissions": "npx zero-deploy-permissions -p src/db/zSchema.ts",
		"dev": "vite dev",
		"dev:zero-cache": "zero-cache-dev -p src/db/zSchema.ts | python format_logs.py",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"migrate": "drizzle-kit migrate",
		"migrate:generate": "drizzle-kit generate",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview",
		"start": "node build",
		"test": "npm run test:unit -- --run",
		"test:unit": "vitest",
		"sso": "/usr/local/bin/aws sso login --sso-session=Looped"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.21.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.17.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.9",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@types/pg": "^8.11.11",
		"daisyui": "5.0.0-beta.8",
		"drizzle-kit": "^0.30.5",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.2",
		"eslint-plugin-svelte": "^3.0.2",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.5.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.20.5",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.9",
		"tsx": "^4.19.3",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.25.0",
		"vite": "^6.2.0",
		"vitest": "^3.0.7"
	},
	"dependencies": {
		"@ai-sdk/openai": "^1.1.15",
		"@ai-sdk/svelte": "^1.1.19",
		"@auth0/auth0-spa-js": "^2.1.3",
		"@rocicorp/zero": "0.16.2025022602",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"ai": "^4.1.47",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.40.0",
		"drizzle-zero": "^0.5.1",
		"nanoid": "^5.1.2",
		"pg": "^8.13.3",
		"postgres": "^3.4.5",
		"sst": "3.9.20",
		"svelte-french-toast": "^1.2.0",
		"svelte-kit-sst": "2.43.5",
		"svelte-markdown": "^0.4.1",
		"zero-svelte": "^0.2.0",
		"zod": "^3.24.2"
	},
	"trustedDependencies": [
		"@rocicorp/zero-sqlite3"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"@rocicorp/zero-sqlite3"
		]
	}
}
